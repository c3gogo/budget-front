right: theme.spacing(2)\n  }\n}))\n\nconst Alert = (props) => {\n  return <MuiAlert elevation={6} variant=\"filled\" {...props} />\n}\nexport default function AccountsPage(props) {\n  const classes = useStyles()\n  const [open, setOpen] = useState(false)\n  const [name, setName] = useState(\"\")\n  const [addAccount] = useMutation(ADD_ACCOUNT, { refetchQueries: [{ query: GET_ACCOUNTS }] })\n  const handleOpenModal = () => {\n    setOpen(true)\n  }\n\n  const handleCloseModal = () => {\n    setOpen(false)\n  }\n\n  const handleSubmit = (e) => {\n    e.preventDefault()\n    addAccount({ variables: { name }})\n    setName(\"\")\n    setOpen(false)\n  }\n\n  return (\n    <div>\n      <CssBaseline />\n      <Container fixed>\n        {\n          props.accounts ? <AccountsList categories={props.accounts} /> : null\n        }\n        <Dialog open={open} onClose={handleCloseModal} aria-labelledby=\"form-dialog-title\">\n          <DialogTitle id=\"form-dialog-title\">Add an account</DialogTitle>\n          <DialogContent>\n            <TextField autoFocus margin=\"dense\" id=\"name\" label=\"Name\" fullWidth onChange={(e) => setName(e.target.value)} value={name} />\n          </DialogContent>\n          <DialogActions>\n            <Button onClick={handleCloseModal} color=\"primary\">\n              Cancel\n            </Button>\n            <Button onClick={handleSubmit} color=\"primary\">\n              Add\n            </Button>\n          </DialogActions>\n        </Dialog>\n        <Fab color=\"primary\" className={classes.fab} aria-label=\"add\" onClick={handleOpenModal}>\n          <AddIcon />\n        </Fab>\n      </Container>\n    </div>\n  )\n}\n","/home/corentin/Documents/Projets/budget-app/front/src/components/Account/AccountsList.js",[],"/home/corentin/Documents/Projets/budget-app/front/src/components/Account/Account.js",["51"],"import React from 'react'\nimport Card from '@material-ui/core/Card'\nimport CardHeader from '@material-ui/core/CardHeader'\nimpor